{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.renderCharacters": false,
    "workbench.colorTheme": "Solarized Dark+",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "reason_language_server.useOldDuneProcess": false,
    "editor.cursorStyle": "line-thin",
    "emmet.includeLanguages": {
        "reason": "javascriptreact"
    },
    "editor.minimap.enabled": false,
    "go.useLanguageServer": true,
    "svelte.language-server.runtime": "/usr/local/bin/node",
    "editor.fontFamily": "'Cascadia Code PL', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vsicons.dontShowNewVersionMessage": true,
}